<div class="container-fluid p-0">
    <div class="row m-0  ">
        <nav class="navbar navbar-expand-lg w-100">
            <img src="assets/img/logo-app.png" class="rounded-circle navbar-brand" width="100" height="100">
            <div class="">
                <spam class="navbar-text">Welcome: {{userName}}</spam>
            </div>
            <div class="ms-auto">
                <button class="btn btn-success w-100">Add a Task</button>
            </div>
            <div>
               <a class="btn btn-outline-primary m-1" href="/logout">Logout<span class="sr-only"></span></a>
            </div>
    </div>
    </nav>
</div>
</div>
{{#each listTask}}
<div class="container-custom container-md col-md-12 shadow-lg mt-3  p-5 min-vh-50">
    <div class="row justify-content-center">
        <div class="col-md-7 card me-1 mb-2 border shadow-lg justify-content-center">{{title}}</div>
        <div class="col-md-1 me-1  d-flex justify-content-center">
            {{#if completada}}
               <input class="form-check-input mt-3 p-3 mb-2 border shadow-lg" type="checkbox" checked> 
            {{else}}
            <input class="form-check-input mt-3 p-3 mb-2 border shadow-lg" type="checkbox">
            {{/if}}
        </div>
        <div class="col-md-3 p-3 mw 100 d-flex justify-content-center border shadow-lg">
            <div class="btn btn-info me-1">update</div>
            <div class="btn btn-danger">Delete</div>
        </div>
        
    </div>
</div>
</div>
{{/each}}